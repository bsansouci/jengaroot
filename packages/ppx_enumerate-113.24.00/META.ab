version = "$(pkg_version)"
description = "Generate a list containing all values of a finite type"
requires(-ppx_driver) = "ppx_core.for_ppx_deriving ppx_type_conv.for_ppx_deriving"
requires(ppx_driver) = "ppx_core ppx_type_conv"
archive(ppx_driver, byte          ) = "ppx_enumerate.cma"
archive(ppx_driver, native        ) = "ppx_enumerate.cmxa"
archive(ppx_driver, native, plugin) = "ppx_enumerate.cmxs"
exists_if = "ppx_enumerate.cma"
ppxopt(-ppx_driver) = "ppx_deriving,./ppx_enumerate.cma"
